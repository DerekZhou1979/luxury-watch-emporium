{
  "name": "chronolab-watch-emporium",
  "displayName": "汉时辰制 ChronoLab",
  "description": "汉时辰制ChronoLab - 中华时计精工，传承时间艺术。用户可以浏览产品、查看详情、个人定制、管理购物车等功能。",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "keywords": [
    "chronolab",
    "汉时辰制",
    "luxury-watches",
    "ecommerce",
    "customization",
    "react",
    "typescript",
    "vite",
    "chinese-timepiece"
  ],
  "author": {
    "name": "Derek Team"
  },
  "license": "MIT",
  "homepage": "https://DerekZhou1979.github.io/luxury-watch-emporium",
  "repository": {
    "type": "git",
    "url": "https://github.com/DerekZhou1979/luxury-watch-emporium.git"
  },
  "bugs": {
    "url": "https://github.com/DerekZhou1979/luxury-watch-emporium/issues"
  },
  "metadata": {
    "requestFramePermissions": []
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && mkdir -p dist/database && cp database/seagull-watch-db.json dist/database/",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@google/genai": "^1.3.0",
    "@tailwindcss/vite": "^4.1.10",
    "antd": "^5.26.6",
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
