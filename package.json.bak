{
  "name": "luxury-watch-emporium",
  "displayName": "Luxury Watch Emporium",
  "description": "An elegant e-commerce web application showcasing a fictional luxury watch brand, ChronoCraft. Users can browse products, view details, manage a shopping cart, and learn about the brand. Features AI-generated stylistic insights for products.",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "keywords": [
    "luxury-watches",
    "ecommerce",
    "react",
    "typescript",
    "vite",
    "ai-insights",
    "chronocraft"
  ],
  "author": {
    "name": "Derek Team"
  },
  "license": "MIT",
  "homepage": "https://DerekZhou1979.github.io/luxury-watch-emporium",
  "repository": {
    "type": "git",
    "url": "https://github.com/DerekZhou1979/luxury-watch-emporium.git"
  },
  "bugs": {
    "url": "https://github.com/DerekZhou1979/luxury-watch-emporium/issues"
  },
  "metadata": {
    "requestFramePermissions": []
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run copy-database",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "clean": "rm -rf dist",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@google/genai": "^1.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "gh-pages": "^6.3.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
